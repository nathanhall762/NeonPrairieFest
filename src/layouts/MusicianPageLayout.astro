import { getData } from '../firebase';
import BaseLayout from './BaseLayout.astro';
import CardContainer, {
  type Musician,
} from '../components/MusicianCard/CardContainer';
import MusicianPage from '../components/MusicianPage/MusicianPage.tsx';

const { musicianID } = Astro.props;

const musicianData = (await getData('musicians', musicianID)) as Musician[];

console.log(`musicianData`, musicianData);
---

<BaseLayout>
  <MusicianPage
    musician={musicianData.find((musician) => musician.id === musicianID)}
    client:load
  />
</BaseLayout>

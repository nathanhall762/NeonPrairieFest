---
import type { DocumentData } from 'firebase/firestore';
import { getData } from '../firebase';
import BaseLayout from './BaseLayout.astro';
import CardContainer, { type Musician } from '../components/MusicianCard/CardContainer';

export interface Item {
  type: string;
  value: any;
}

const musicians = await getData('musicians') as Musician[];
---

<BaseLayout>
    <CardContainer musicians={musicians} client:load/>
</BaseLayout>
